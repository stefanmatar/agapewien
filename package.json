    "mini-css-extract-plugin": "^0.4.4",
    "node-sass": "^4.9.4",
    "npm-run-all": "^4.1.3",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-loader": "^3.0.0",
    "precss": "^3.1.2",
    "raw-loader": "^0.5.1",
    "sass-loader": "^7.1.0",
    "stylelint": "^9.6.0",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "webpack": "^4.23.1",
    "webpack-cli": "^3.1.2",
