{
  "name": "bootpack",
  "version": "0.9.0",
  "author": "ZachTRice",
  "description": "A webpack and bootstrap boilerplate template for building web pages quickly.",
  "license": "MIT",
  "homepage": "https://github.com/bootpack/bootpack#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bootpack/bootpack.git"
  },
  "bugs": {
    "url": "https://github.com/bootpack/bootpack/issues"
  },
  "keywords": [
    "webpack",
    "template",
    "webpage",
    "starter",
    "website"
  ],
  "scripts": {
    "build": "webpack --mode=production",
    "watch": "webpack --watch --mode=development",
    "start": "node server.js",
    "start-dev": "webpack-dev-server --mode=development --open",
    "test": "run-s lint:*",
    "lint": "run-s lint:*",
    "lint:css": "stylelint ./src/css/*.css",
    "lint:js": "eslint .",
    "generate:images": "node tools/generate-images.js",
    "heroku-postbuild": "npm run build"
  },
  "dependencies": {
    "bootstrap": "^4.1.3",
    "jquery": "^3.3.1",
    "npm": "^6.4.1",
    "popper.js": "^1.14.4"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.4",
    "babel-preset-env": "^1.6.1",
    "clean-webpack-plugin": "^0.1.19",
    "compression-webpack-plugin": "^2.0.0",
    "copy-webpack-plugin": "^4.5.4",
    "css-loader": "^1.0.0",
    "eslint": "^5.7.0",
    "eslint-config-import": "^0.13.0",
    "eslint-config-node": "^4.0.0",
    "eslint-config-promise": "^1.2.6",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-node": "^7.0.1",
